let lift = +prompt("Виберіть поверх на який ви б хотіли піднятися?");

if (lift < 1 || lift > 102) {
    console.log("Невірний поверх");
} else if (lift < 24 || lift > 39) {
    console.log("ок");
} else if (lift >= 24 || lift <= 39) {

    let passsword = +prompt("Введіть пароль");

    if (passsword === 1337) {
        console.log("ок");
    } else {
        console.log("Невірний пароль");
    }
}
